{
  "extends": [
    "gts/tsconfig-google.json",
    "@vue/tsconfig/tsconfig.dom.json"
  ],
  "include": [
    "env.d.ts",
    "src/**/*.ts",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "allowSyntheticDefaultImports": true,
    "checkJs": true,
    "composite": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "lib": [
      "DOM",
      "ESNext"
    ],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "outDir": "dist",
    "target": "ESNext"
  }
}
